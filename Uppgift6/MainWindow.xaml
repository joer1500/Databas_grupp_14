<Window x:Class="Uppgift6.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Uppgift6"
        mc:Ignorable="d"
        Title="Startsida" Height="450" Width="800" WindowStartupLocation="CenterScreen">
    <Grid>
        <Button x:Name="btnStaff" Content="Anställda" HorizontalAlignment="Left" Margin="503,164,0,0" VerticalAlignment="Top" Width="173" Height="44" Click="btnStaff_Click"/>
        <Label x:Name="label" Content="Logga in som vårdnadshavare" HorizontalAlignment="Left" Margin="89,118,0,0" VerticalAlignment="Top"/>
        <Button x:Name="btnGuardian" Content="Vårdnadshavare" HorizontalAlignment="Left" Margin="89,164,0,0" VerticalAlignment="Top" Width="173" Height="44" Click="BtnGuardian_Click"/>
        <Separator HorizontalAlignment="Left" Height="16" Margin="163,189,0,0" VerticalAlignment="Top" Width="444" RenderTransformOrigin="0.5,0.5">
            <Separator.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="90"/>
                    <TranslateTransform/>
                </TransformGroup>
            </Separator.RenderTransform>
        </Separator>
        <Label x:Name="label_Copy" Content="Logga in som personal" HorizontalAlignment="Left" Margin="503,118,0,0" VerticalAlignment="Top"/>
        <ListView x:Name="listView" HorizontalAlignment="Left" Height="144" Margin="45,244,0,0" VerticalAlignment="Top" Width="290">
            <ListView.View>
                <GridView>
                    <GridViewColumn/>
                </GridView>
            </ListView.View>
        </ListView>

    </Grid>
</Window>
